<div class='theme-header'>
  <%= render :partial => "ratings/rate", :locals => { :asset => theme } %>  
  <h2><%= theme.name -%></h2>
  <h3>by <%= theme.creator -%></h3>
  <div class='clear'></div>
  <ul class='subnav'>
    <li><%= link_to 'Create a new theme based on this one', copy_theme_path(theme) -%></li>
  </ul>
</div>
